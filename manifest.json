{
    "name": "Open Tistory as cache",
    "description" : "Open Tistory blog as a web cache directly.",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["redirect-to-cache.js"]
    },
    "permissions": [
        "tabs",
        "webNavigation"
    ]
}